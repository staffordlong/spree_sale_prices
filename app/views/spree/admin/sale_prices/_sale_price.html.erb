<tr data-hook="products_row" id="<%= spree_dom_id(sale_price) %>" class="spree-product-sale-price">
  <td class="text-center"><%= sale_price.active? ? 'Yes' : 'No' %></td>
  <td><%= sale_price.display_price.to_html %></td>
  <td><%= l(sale_price.start_at.to_date) if sale_price.start_at %></td>
  <td><%= l(sale_price.end_at.to_date) if sale_price.end_at %></td>
  <td class="text-center"><%= sale_price.enabled %></td>
  <td class="actions">
    <%= link_to_delete sale_price, { url: admin_product_sale_price_url(product, sale_price.id), no_text: true } %>
  </td>
</tr>